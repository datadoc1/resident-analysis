$(document).ready(function () {

    //constants
    var initStateHideDivCount = 28;
    var initProgramSettingHideDivCount = 28;

    //state 
    var divCount2 = $("#StateInnerDiv div").size();
    $('#StateInnerDiv div:lt(' + initStateHideDivCount + ')').show();
    $('#StateOuterDiv #loadMore').click(function () {
        $('#StateInnerDiv div:lt(' + divCount2 + ')').show();
        $('#StateOuterDiv #showLess').show();
        $('#StateOuterDiv #loadMore').hide();
    });
    $('#StateOuterDiv #showLess').click(function () {
        $('#StateInnerDiv div').hide();
        $('#StateInnerDiv div:lt(' + initStateHideDivCount + ')').show();
        $('#StateOuterDiv #showLess').hide();
        $('#StateOuterDiv #loadMore').show();
    });

    //program setting 
    var divCount3 = $("#ProgramSettingInnerDiv div").size();
    $('#ProgramSettingInnerDiv div:lt(' + initProgramSettingHideDivCount + ')').show();
    $('#ProgramSettingOuterDiv #loadMore').click(function () {
        $('#ProgramSettingInnerDiv div:lt(' + divCount3 + ')').show();
        $('#ProgramSettingOuterDiv #showLess').show();
        $('#ProgramSettingOuterDiv #loadMore').hide();
    });
    $('#ProgramSettingOuterDiv #showLess').click(function () {
        $('#ProgramSettingInnerDiv div').hide();
        $('#ProgramSettingInnerDiv div:lt(' + initProgramSettingHideDivCount + ')').show();
        $('#ProgramSettingOuterDiv #showLess').hide();
        $('#ProgramSettingOuterDiv #loadMore').show();
    });

    //Visa setting
    var divCount4 = $("#VisaAcceptedInnerDiv div").size();
    $('#VisaAcceptedInnerDiv div:lt(' + initProgramSettingHideDivCount + ')').show();
    $('#VisaAcceptedOuterDiv #loadMore').click(function () {
        $('#VisaAcceptedInnerDiv div:lt(' + divCount3 + ')').show();
        $('#VisaAcceptedOuterDiv #showLess').show();
        $('#VisaAcceptedOuterDiv #loadMore').hide();
    });
    $('#VisaAcceptedOuterDiv #showLess').click(function () {
        $('#VisaAcceptedInnerDiv div').hide();
        $('#VisaAcceptedInnerDiv div:lt(' + initProgramSettingHideDivCount + ')').show();
        $('#VisaAcceptedOuterDiv #showLess').hide();
        $('#VisaAcceptedOuterDiv #loadMore').show();
    });

});